@model Tamplate_2.Models.C_Images

@{
    ViewBag.Title = "Delete";
}

<br /><br /><br /><br />


<div class="container-fluid text-center ">

    <h2>photos page</h2><br>
    <h3 style="color:red;">Delete</h3>
    <h4>Are you sure you want to delete this?</h4>

    <div class="row text-center" style="border:solid;  padding-top:10px; margin:0px 5px 0px 5px;">

        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image title</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Title)

                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image alt</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Alt)

                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image Width</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Width)

                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image Hight</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Hight)

                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image Link</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Link)

                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image Section_Id</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Section_Id)

                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image C_Description</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Description)

                </div>
            </div>
        </div>

        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image Order no</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Order)

                </div>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="thumbnail">
                <div class="editor-label">
                    <h4>image Display state</h4>
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => model.C_Display)

                </div>
            </div>
        </div>
    </div>@*end of row*@

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

    
        <div class="row">
            <div class="col-sm-12">
                <p>
                    <input type="submit" value="Delete" /> |
                  
                </p>
            </div>
        </div>
    <div class="row">
        <div class="col-sm-12">
            <p>
                @Html.ActionLink("Back to List", "Index")
            </p>
        </div>
    </div>

    }

</div>@*end of container*@






       
      
