@model IEnumerable<Tamplate_2.Models.C_Section_Content>

@{
    ViewBag.Title = "Index";
}




<div class="container-fluid text-center bg-grey">
    <div class="row text-center">
        <div class="col-sm-4"></div>
        <div class="col-sm-4">

            @if (ViewBag.sectionid != 1)
            {

                <h4>you can edit delete and @Html.ActionLink("create new content", "Create", "Section_content", new { myid = @ViewBag.sectionid }, null) from here</h4><br />

            }

            <br />
        </div>
    </div>
    @*-----------------------------------------------------------------------------------------------*@
    @{string H = "";}
    
    @if (ViewBag.sectionid == 1)//header
    {


        <div class="row add-top add-bottom-services">



            @foreach (var n in Model)
            {
                <div class="col-lg-12">
                    <header style="background:#364b60 url(../../@Html.DisplayFor(modelItem => n.C_Images7.C_Link)) ; text-align: center;  color: #FCE301;   background-attachment: scroll;   background-position: center center;   background-repeat: none;   -webkit-background-size: cover;   -moz-background-size: cover;   background-size: cover;  -o-background-size: cover; padding-top:300px;">

                        <div id="top" class="container">
                            <div class="intro-text">
                                <div class="intro-lead-in">@Html.DisplayFor(modelItem => n.C_p_txt1)</div>
                                <div class="intro-heading">@Html.DisplayFor(modelItem => n.C_Txt_1)</div>
                                <a href="#" class="btn btn-primary btn-lg">let us start</a>

                            </div>
                        </div>
                    </header>
                </div>
                <div class="col-md-12">

                    @Html.ActionLink("Edit", "Edit", new { id = n.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = n.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = n.Id })
                    <br /><br />
                </div>


            }




        </div>
        //end of if for section 2
    }

    @*-----------------------------------------------------------------------------------------------*@


    else if (ViewBag.sectionid == 2)//services
    {


        <div class="row add-top add-bottom-services">



            @foreach (var n in Model)
            {


                <div class="col-md-4">
                    <span>
                        <a> <i class="@Html.DisplayFor(modelItem => n.C_Txt_7) icon-a"></i></a>
                    </span>

                    <h2 class="service-heading">@Html.DisplayFor(modelItem => n.C_Heading)</h2>
                    <p class="text-muted">@Html.DisplayFor(modelItem => n.C_p_txt1).</p>

              <br />

                    @Html.ActionLink("Edit", "Edit", new { id = n.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = n.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = n.Id })
                    <br /><br />
                </div>


            }




        </div>
        //end of if for section 2
    }

    @*-----------------------------------------------------------------------------------------------*@


    else if (ViewBag.sectionid == 3)//prtfolio
    {


        <div class="row add-top add-bottom-services">

            <nav id="intro-nav">
                <ul class="ca-menu">

                    @foreach (var n in Model)
                    {


                        <div class="col-md-4 col-sm-6 portfolio-item">

                            <a href="@Url.Action("Index", "portfolio" , new { searchtrim=n.C_So_Search_Trim_2})" class="portfolio-link" data-toggle="modal">



                                <div class="portfolio-hover">
                                    <div class="portfolio-hover-content">
                                        <i class="@Html.DisplayFor(modelItem => n.C_Txt_7)"></i>@*font asowme*@
                                    </div>
                                </div>

                                <img src="../../@Html.DisplayFor(modelItem => n.C_Images.C_Link)" title="@Html.DisplayFor(modelItem => n.C_Images.C_Title)" class="img-responsive" alt="@Html.DisplayFor(modelItem => n.C_Images.C_Alt)" width="360" />

                            </a>
                            <div class="portfolio-caption">
                                <h4>@Html.DisplayFor(modelItem => n.C_H4_txt1)</h4>
                                <p class="text-muted">@Html.DisplayFor(modelItem => n.C_p_txt1)</p>
                            </div>
                            <br />

                            @Html.ActionLink("Edit", "Edit", new { id = n.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = n.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = n.Id })
                            <br /><br />
                        </div>

                    }

                </ul>

            </nav>


        </div>
        //end of if for section 3 portfolio
    }

    @*-----------------------------------------------------------------------------------------------*@
    else if (ViewBag.sectionid == 4)//EVENTS
    {


        <div class="row add-top ">

            <div class="col-lg-12">
                <ul class="timeline">

                    @foreach (var n in Model)
                    {



    


                        <li class="@Html.DisplayFor(modelItem => n.C_L_To_R)">
                            <div class="timeline-image">
                                <a href="@Html.DisplayFor(modelItem => n.C_Url_1)" class="timeline-link">
                                    <img class="img-circle img-responsive" src="../../@Html.DisplayFor(modelItem => n.C_Images.C_Link)" width="175" height="175" alt="@Html.DisplayFor(modelItem => n.C_Images.C_Alt)">
                                </a>
                            </div>
                            <div class="timeline-panel">
                                <div class="timeline-heading">
                                    <h4>@Html.DisplayFor(modelItem => n.C_H4_txt1)</h4>
                                    <h4 class="subheading">@Html.DisplayFor(modelItem => n.C_H4_txt2)</h4>
                                </div>
                                <div class="timeline-body">
                                    <p class="text-muted">@Html.DisplayFor(modelItem => n.C_p_txt1)</p>
                                </div>
                            </div>
                        </li>
                        <div>
                            @Html.ActionLink("Edit", "Edit", new { id = n.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = n.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = n.Id })
                            <br />
                            <br />
                        </div>
 
                    }




                </ul>


            </div>

        </div>
        //end of if for section 4 EVENTS
    }

    @*-----------------------------------------------------------------------------------------------*@


    @*------contact section----------*@
    else if (ViewBag.sectionid == 5)
    {
        <div class="row">
            @foreach (var n in Model)
            {


                <div id="map-outer" class="col-md-12">
                    <div id="address" class="col-md-4">
                        <h2>@Html.DisplayFor(modelItem => n.C_Heading)</h2>
                        <address>
                            <strong>@Html.DisplayFor(modelItem => n.C_Sub_Heading)</strong><br>
                            @Html.DisplayFor(modelItem => n.C_p_txt1)<br>
                            @Html.DisplayFor(modelItem => n.C_p_txt2)<br>
                            @Html.DisplayFor(modelItem => n.C_p_txt3)<br>
                            @Html.DisplayFor(modelItem => n.C_p_txt4)
                            <br>
                            <abbr>@Html.DisplayFor(modelItem => n.C_Txt_1)</abbr> @Html.DisplayFor(modelItem => n.C_Txt_2)
                            <br>
                            <abbr>@Html.DisplayFor(modelItem => n.C_Txt_3)</abbr> @Html.DisplayFor(modelItem => n.C_Txt_4)

                        </address>
                    </div>
                    <div id="map-container" class="col-md-8">

                        <div id="map_canvas" style=" height:250px; text-align:center;">map for:  @Html.DisplayFor(modelItem => n.C_Url_4)</div>
                    </div>
                </div><!-- /map-outer -->

                <div>
                    @Html.ActionLink("Edit", "Edit", new { id = n.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = n.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = n.Id })
                    <br /><br />
                </div>


            } @*Endof for each*@
        </div>//end of row

    } //end of if section 5 contact

    @*-----------------------------------------------------------------------------------------------*@
    @*------facebook section----------*@

    else if (ViewBag.sectionid == 6)
    {
        int VideoNo = 1;
        <div class="row add-top add-bottom-services">

            @foreach (var n in Model)
            {
                <div class="col-lg-12" style="border:5px solid black">
                    @if (n.C_So_Search_Trim_1 == "Posts")
                    {
                        <h4>page`s posts  </h4>

                        <img src="~/Images/main/posts.png" />

                    }
                    @if (n.C_So_Search_Trim_1 == "Videos")
                    {
                        <h4>page`s videos no : @VideoNo </h4>
                        <img src="~/Images/main/videos.png" />
                        VideoNo = VideoNo + 1;
                    }
                    @if (n.C_So_Search_Trim_1 == "Comments")
                    {
                        <h4>page`s Comments   </h4>
                        <img src="~/Images/main/comments.png" />

                    }
                    @if (n.C_So_Search_Trim_1 == "Photos")
                    {
                        <h4>page`s Photos  </h4>
                        <img src="~/Images/main/photos.png" />

                    }
                </div>
                <div>
                    @Html.ActionLink("Edit", "Edit", new { id = n.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = n.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = n.Id })
                    <br /><br />
                </div>

            } @*Endof for each*@
        </div>

    }

    @*-----------------------------------------------------------------------------------------------*@
    else if (ViewBag.sectionid == 7)//footer
    {


        <div class="row modal-footer">
            <div class="col-md-6">
                <ul class="list-inline social-buttons">

                    @foreach (var n in Model)
                    {

                        <li>
                            <a href="@n.C_Url_1"><i class="@n.C_Txt_7"></i></a>
                        </li>
                <div>
                        @Html.ActionLink("Edit", "Edit", new { id = n.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = n.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = n.Id })
                    <br /><br />
                </div>
                }

                </ul>
            </div>
        </div>
  
        //end of if for section 7 footer
    }

    @*-----------------------------------------------------------------------------------------------*@
    @*------News section----------*@
    else if (ViewBag.sectionid == 9)
    {
        <div class="row">
            @foreach (var n in Model)
            {
                <div class="col-lg-4 news-block">
                    <div class="row-fluid">
                        <article class="span12 news-img">
                            <div class="news-date" style="overflow: hidden; display: block;">
                                <h2>@Html.DisplayFor(modelItem => n.C_H2_txt1)</h2>
                                <h4>@Html.DisplayFor(modelItem => n.C_H4_txt1)</h4>
                                <h4>@Html.DisplayFor(modelItem => n.C_H4_txt2)</h4>
                            </div>
                            <img class="news-img" alt="@Html.DisplayFor(modelItem => n.C_Images.C_Alt)" title="@Html.DisplayFor(modelItem => n.C_Images.C_Title)" src="../../@Html.DisplayFor(modelItem => n.C_Images.C_Link)">
                        </article>
                    </div>
                    <div>
                        <div class="row-fluid">
                            <article class="span12 news-specs">
                                <nav>
                                    <a href="#">

                                        <span class="news-specs-info">&nbsp; <i class="@Html.DisplayFor(modelItem => n.C_Txt_7)"></i> &nbsp;@Html.DisplayFor(modelItem => n.C_Txt_1)</span>

                                    </a>
                                    <a href="#">
                                        <span class="news-specs-info">&nbsp; <i class="@Html.DisplayFor(modelItem => n.C_Txt_8)"></i> &nbsp;@Html.DisplayFor(modelItem => n.C_Txt_2)</span>

                                    </a>

                                </nav>
                            </article>
                        </div>
                    </div>
                    <div class="row-fluid">
                        <article class="span12 news-block-content">
                            <h3 class="news-heading"><span>@Html.DisplayFor(modelItem => n.C_Heading)</span></h3>
                            <p>@Html.DisplayFor(modelItem => n.C_p_txt1)</p>
                            <a href="#" class="btn btn-renova-alt">@Html.DisplayFor(modelItem => n.C_Txt_3)</a>

                        </article>
                    </div>
                    @Html.ActionLink("Edit", "Edit", new { id = n.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = n.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = n.Id })
                    <br /><br />
                </div>


            } @*Endof for each*@
        </div>//end of row
        //end of if section 9
    }

    @*-----------------------------------------------------------------------------------------------*@



    <div class="row text-center">
        <div class="col-sm-4"></div>
        <div class="col-sm-4">
            <div class="thumbnail">
                @foreach (var item in Model.Take(1))
                {
                    if (item.C_Section_Id != 1)
                    {


                        <h3>@Html.ActionLink("create new content", "Create", "Section_content", new { myid = item.C_Section_Id }, null)</h3>
                    }
                }

            </div>
        </div>
    </div>@*end of row 3*@


    <

    </div>@*end of container*@







